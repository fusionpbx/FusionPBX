<context name="{v_context}">
        <extension name="call-limit" number="" continue="true" app_uuid="4670c44c-45dd-4bae-97ba-b0dfe0aca639" enabled="false">
                <condition field="${user_exists}" expression="^true$">
                        <anti-action application="set" data="domain_name={v_context}" inline="true"/>
                        <anti-action application="set" data="max_calls=3" inline="true"/>
                        <anti-action application="limit" data="hash inbound ${domain_name} ${max_calls} !USER_BUSY"/>
                </condition>
        </extension>
</context>
